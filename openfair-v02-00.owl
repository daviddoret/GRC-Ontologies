                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/daviddoret/ontologies/2017/6/openfair-v2#Factor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/daviddoret/ontologies/2017/6/openfair-v2#hasFactorValue"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
